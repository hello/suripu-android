<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- PARTIAL -->

    <permission
        android:name="is.hello.sense.debug.permission.C2D_MESSAGE"
        android:protectionLevel="signature"/>
    <uses-permission android:name="is.hello.sense.debug.permission.C2D_MESSAGE"/>

    <application>

        <activity
            android:name="is.hello.sense.debug.EnvironmentActivity"
            android:theme="@style/AppTheme.Dialog.Simple"/>
        <activity
            android:name="is.hello.sense.debug.TestActivity"
            android:theme="@style/AppTheme.AppCompat"/>

        <activity
            android:name="is.hello.sense.ui.activities.DebugActivity"
            android:label="@string/activity_debug"
            android:launchMode="singleTop"/>

        <activity
            android:name="net.hockeyapp.android.UpdateActivity"/>

        <activity
            android:name="net.hockeyapp.android.FeedbackActivity"/>

        <activity
            android:name="is.hello.sense.debug.PiruPeaActivity"
            android:label="Piru-Pea"
            android:screenOrientation="portrait"/>

        <activity
            android:name="is.hello.sense.debug.WelcomeDialogsActivity"/>

        <receiver
            android:name="is.hello.sense.notifications.NotificationReceiver"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <category android:name="is.hello.sense.debug"/>
            </intent-filter>
        </receiver>

        <meta-data
            android:name="com.mixpanel.android.MPConfig.ResourcePackageName"
            android:value="is.hello.sense"/>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/FACEBOOK_APP_ID"/>

    </application>

</manifest>
