<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Specific Tinting -->

    <style name="TintOverride" />

    <style name="TintOverride.SleepScore">
        <item name="android:colorEdgeEffect">?android:attr/tint</item>
    </style>

    <style name="TintOverride.SleepScore.Ideal">
        <item name="android:tint">@color/sensor_ideal</item>
    </style>

    <style name="TintOverride.SleepScore.Warning">
        <item name="android:tint">@color/sensor_warning</item>
    </style>

    <style name="TintOverride.SleepScore.Alert">
        <item name="android:tint">@color/sensor_alert</item>
    </style>

    <style name="FacebookTheme" parent="FacebookTheme.Base">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="AppTheme.ProgressBar.Horizontal" parent="android:Widget.Holo.ProgressBar.Horizontal">
        <item name="android:minWidth">164dp</item>
        <item name="android:maxWidth">164dp</item>
        <item name="android:max">100</item>
        <item name="android:secondaryProgress">100</item>
        <item name="android:secondaryProgressTint">@color/gray3</item>
    </style>
</resources>